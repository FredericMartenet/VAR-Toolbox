\@writefile{toc}{\contentsline {subsection}{\numberline {3.2}Load \& Plot Data {\color  {note} {\small {[Matlab]}}}}{4}{subsection.3.2}\protected@file@percent }
\@writefile{tdo}{\contentsline {todo}{\color@fb@x {}{white}{}{script!80}{\leavevmode {\color  {script!80}o}}\ \ttfamily  \hspace  {1mm}\leavevmode {\color  {matlabgreen}\%}\leavevmode {\color  {matlabgreen}\% 1. LOAD \& PLOT DATA }\\ \hspace  {1mm}\leavevmode {\color  {matlabgreen}\%-------------------------------------------------------------------------- }\\ \hspace  {1mm}\leavevmode {\color  {matlabgreen}\% Load }\\ \hspace  {1mm}[xlsdata, xlstext] = xlsread(\leavevmode {\color  {matlabpurple}'GK2015\_Data.xlsx'},\leavevmode {\color  {matlabpurple}'VAR\_data'}); \\ \hspace  {1mm}data = Num2NaN(xlsdata(:,3:end)); \\ \hspace  {1mm}vnames = xlstext(1,3:end); \\ \hspace  {1mm}\leavevmode {\color  {matlabblue}for} ii=1:length(vnames) \\ \hspace  {1mm}\hspace  {5mm} DATA.(vnames\{ii\}) = data(:,ii); \\ \hspace  {1mm}\leavevmode {\color  {matlabblue}end} \\ \hspace  {1mm}year = xlsdata(1,1); \\ \hspace  {1mm}month = xlsdata(1,2); \\ \hspace  {1mm}\leavevmode {\color  {matlabgreen}\% Observations }\\ \hspace  {1mm}nobs = size(data,1); \\ \hspace  {1mm}\leavevmode {\color  {matlabgreen}\% Set endogenous }\\ \hspace  {1mm}VARvnames = \{\leavevmode {\color  {matlabpurple}'gs1'},\leavevmode {\color  {matlabpurple}'logcpi'},\leavevmode {\color  {matlabpurple}'logip'},\leavevmode {\color  {matlabpurple}'ebp'}\}; \\ \hspace  {1mm}VARvnames\_long = \{\leavevmode {\color  {matlabpurple}'Policy rate'},\leavevmode {\color  {matlabpurple}'CPI'},\leavevmode {\color  {matlabpurple}'Industrial Production'},\leavevmode {\color  {matlabpurple}'EBP'}\}; \\ \hspace  {1mm}VARnvar = length(VARvnames); \\ \hspace  {1mm}\leavevmode {\color  {matlabgreen}\% Create matrices of variables to be used in the VAR }\\ \hspace  {1mm}ENDO = nan(nobs,VARnvar); \\ \hspace  {1mm}\leavevmode {\color  {matlabblue}for} ii=1:VARnvar \\ \hspace  {1mm}\hspace  {5mm} ENDO(:,ii) = DATA.(VARvnames\{ii\}); \\ \hspace  {1mm}\leavevmode {\color  {matlabblue}end} \\ }{4}{section*.2}\protected@file@percent }
\pgfsyspdfmark {pgfid2}{19562754}{27198570}
\@writefile{tdo}{\contentsline {todo}{\color@fb@x {}{white}{}{script!80}{\leavevmode {\color  {script!80}o}}\ \ttfamily  \hspace  {1mm}\leavevmode {\color  {matlabgreen}\% Open a figure of the desired size }\\ \hspace  {1mm}FigSize(28,16) \\ \hspace  {1mm}\leavevmode {\color  {matlabblue}for} ii=1:VARnvar \\ \hspace  {1mm}\hspace  {5mm} subplot(2,2,ii) \\ \hspace  {1mm}\hspace  {5mm} H(ii) = plot(ENDO(:,ii),\leavevmode {\color  {matlabpurple}'LineWidth'},2,\leavevmode {\color  {matlabpurple}'Color'},cmap(ii)); \\ \hspace  {1mm}\hspace  {5mm} title(VARvnames\_long(ii)); \\ \hspace  {1mm}\hspace  {5mm} DatesPlot(year+(month-1)/12,nobs,6,\leavevmode {\color  {matlabpurple}'m'}) \leavevmode {\color  {matlabgreen}\% Set the x-axis label }\\ \hspace  {1mm}\hspace  {5mm} grid on; \\ \hspace  {1mm}\leavevmode {\color  {matlabblue}end} \\ \hspace  {1mm}\leavevmode {\color  {matlabgreen}\% Legend }\\ \hspace  {1mm}lopt = LegOption; lopt.handle = H; LegSubplot(VARvnames,lopt); FigFont(10); \\ \hspace  {1mm}\leavevmode {\color  {matlabgreen}\% Save }\\ \hspace  {1mm}SaveFigure(\leavevmode {\color  {matlabpurple}'graphics/F1\_PLOT'},1) \\ }{4}{section*.3}\protected@file@percent }
