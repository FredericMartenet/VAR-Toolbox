\@writefile{tdo}{\contentsline {todo}{\color@fb@x {}{white}{}{script!80}{\leavevmode {\color  {script!80}o}}\ \ttfamily  \hspace  {1mm}\leavevmode {\color  {matlabgreen}\%}\leavevmode {\color  {matlabgreen}\% VAR ESTIMATION }\\ \hspace  {1mm}\leavevmode {\color  {matlabgreen}\%-------------------------------------------------------------------------- }\\ \hspace  {1mm}\leavevmode {\color  {matlabgreen}\% Set the deterministic variable in the VAR (1=constant, 2=trend) }\\ \hspace  {1mm}det = 1; \\ \hspace  {1mm}\leavevmode {\color  {matlabgreen}\% Set number of nlags }\\ \hspace  {1mm}nlags = 12; \\ \hspace  {1mm}\leavevmode {\color  {matlabgreen}\% Estimate VAR by OLS }\\ \hspace  {1mm}[VAR, VARopt] = VARmodel(ENDO,nlags,det); \\ \hspace  {1mm}disp(VAR) \\ \hspace  {1mm}disp(VARopt) \\ \hspace  {1mm}\leavevmode {\color  {matlabgreen}\% Add variable names to VARopt }\\ \hspace  {1mm}VARopt.vnames = VARvnames; \\ \hspace  {1mm}VARopt.figname= \leavevmode {\color  {matlabpurple}'graphics/'}; \\ \hspace  {1mm}\leavevmode {\color  {matlabgreen}\% Print at screen and create table }\\ \hspace  {1mm}[TABLE, beta] = VARprint(VAR,VARopt,2); \\ }{6}{section*.4}\protected@file@percent }
